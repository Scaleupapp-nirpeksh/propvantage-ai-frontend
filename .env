# File: .env
# Description: Development environment variables for PropVantage AI frontend
# Version: 1.0 - Development configuration
# Location: .env

# API Configuration - Update this to match your backend
REACT_APP_API_URL1=http://localhost:3000/api
REACT_APP_API_URL=https://api.prop-vantage.com/api
REACT_APP_API_TIMEOUT=30000

# Application Configuration
REACT_APP_NAME=PropVantage AI
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Debug Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug

# Build Configuration
GENERATE_SOURCEMAP=true
DISABLE_ESLINT_PLUGIN=false